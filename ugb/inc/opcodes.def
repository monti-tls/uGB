#define _POP(nn) r(SP, &nn ## l); r(SP+1, &nn ## h); SP += 2;
#define _PUSH(nn) w(SP-1, nn ## h); w(SP-2, nn ## l); SP -= 2;
#define _CALL(addr) _PUSH(PC); PC = addr;

